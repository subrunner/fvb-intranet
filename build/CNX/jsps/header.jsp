--%><%
    String requestUser = request.getRemoteUser();
    User user = (User)request.getAttribute(UserInfo.class.getName()+".user");
    boolean isUserExternal = false;
    // FIXME: why do we need this into the request?
    request.setAttribute("requestUser", requestUser);
    // FIXME: why do we need this into the request?
    request.setAttribute("user", user);

    // Retrieve isUserExternal information from Waltz
    try{
        if (user == null && requestUser != null) {
            DSObject obj = DSProviderFactory.INSTANCE.getProfileProvider().searchUserByExactLoginUserIdMatch(requestUser);
            if (obj != null)
                isUserExternal = obj.is_user_external();
        }
    } catch (DSException e) {
        // FIXME: log or show error page
        System.err.println(e);
    } finally {
        request.setAttribute("isUserExternal", isUserExternal);
    }

    //notification center gatekeeper check
    boolean notificationCenterEnabled = false;
    try
    {
    	notificationCenterEnabled = (requestUser != null && LCGatekeeper.isEnabled(LCSupportedFeature.CONNECTIONS_NOTIFICATION_CENTER, request));
    } catch (Exception e){
        System.err.println(e);
    } finally {
    	request.setAttribute("isNotificationCenterEnabled", notificationCenterEnabled);
    }
